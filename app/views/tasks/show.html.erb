<div class="page-header">
  <h3>Task #<%= @task.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">

      <dt>Module</dt>
      <dd><%= @task.module_id %></dd>


      <dt>Name</dt>
      <dd><%= @task.name %></dd>


      <dt>Description</dt>
      <dd><%= @task.description %></dd>


      <dt>Type</dt>
      <dd><%= @task.type %></dd>


      <dt>Graded</dt>
      <dd><%= @task.graded %></dd>


      <dt>Predecessor</dt>
      <dd><%= @task.predecessor %></dd>


      <dt>Url</dt>
      <dd><%= @task.url %></dd>


    </dl>

    <div class="btn-group btn-group-justified">

      <a href="/tasks" class="btn btn-primary">
        Back
      </a>
      <a href="/tasks/<%= @task.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_task/<%= @task.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>

    </div>
  </div>
</div>
